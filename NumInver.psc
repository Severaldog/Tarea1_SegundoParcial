Algoritmo NumInver
	Dimension  nums[10]
	Para i = 0 Hasta 9 Con Paso 1 Hacer
		Leer nums[i]
	Fin Para
	Para i = 0 Hasta 9 Con Paso 1 Hacer
		Escribir "Números Ingresados: " nums[i]
	Fin Para
	Para i = 9 Hasta 0 Con Paso -1 Hacer
		Escribir "Respuesta: " nums[i]
	Fin Para
	
FinAlgoritmo
